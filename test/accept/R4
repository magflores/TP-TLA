/* Un programa que produzca un HTML con niveles de anidamiento. */
START
CONTAINER
TITLE "Tabla de prueba"
  CONTAINER 
  TABLE id:A1 rowxcol:(2x2)
    ROW
      TEXT "Prueba 1"
      TEXT "Prueba 2"
    END ROW
    ROW
      TEXT "Prueba 3"
      TEXT "Prueba 4"
    END ROW
  END TABLE
  END CONTAINER
TEXT "Tabla 1"
END CONTAINER
END
